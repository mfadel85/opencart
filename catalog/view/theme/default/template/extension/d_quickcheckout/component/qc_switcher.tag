<qc_switcher>
    <label class="ve-switch">
        <input name={opts.name} type="hidden"  value="0">
        <input name={opts.name} type="checkbox" class="ve-input" ref="switcher" data-label-text={opts.data-label-text} value="1" checked={(this.opts.riotValue == 1)}>
        <i></i>
    </label>
   
</qc_switcher>